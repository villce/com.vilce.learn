<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vilce.springboot_vue.mapper.AdminRoleMenuMapper">

    <select id="getRoleMenuAByRid" resultType="com.vilce.springboot_vue.model.po.AdminRoleMenu" >
		SELECT *
		FROM admin_role_menu
		WHERE rid = #{rid}
    </select>

    <delete id="deleteRoleMenuByRid" >
		DELETE
		FROM admin_role_menu
		WHERE rid = #{rid}
    </delete>

    <insert id="addRoleMenu" >
		INSERT INTO admin_role_menu
		(rid,mid)
		 VALUES (#{rid},#{mid})
    </insert>
</mapper>
